{
  "name": "pekral/phpstan-rules",
  "description": "Extensible package of custom rules for PHPStan.",
  "type": "library",
  "keywords": [
    "phpstan",
    "rules",
    "static analysis",
    "coding standard",
    "quality",
    "php"
  ],
  "homepage": "https://github.com/pekral/phpstan-rules",
  "license": ["MIT"],
  "authors": [
    {
      "name": "Petr Kr√°l",
      "email": "kral.petr.88@gmail.com",
      "homepage": "https://github.com/pekral",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "https://github.com/pekral/phpstan-rules/issues",
    "source": "https://github.com/pekral/phpstan-rules"
  },
  "require": {
    "php": "^8.4",
    "phpstan/phpstan": "^2.1.19",
    "phpstan/phpstan-deprecation-rules": "^2.0.3",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan-strict-rules": "^2.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "pekral/phpcs-rules": "^0.4.1"
  },
  "autoload": {
    "psr-4": {
      "Pekral\\PhpstanRules\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
